        <string-array name="juz_0">
            <item>الْحَمْدُ لِلَّـهِ رَبِّ الْعَالَمِينَ</item>
            <item>الم</item>
            <item>وَمِنَ النَّاسِ مَن يَقُولُ</item>
            <item>يَا أَيُّهَا النَّاسُ اعْبُدُوا</item>
            <item>وَإِذْ قَالَ رَبُّكَ لِلْمَلَائِكَةِ</item>
            <item>يَا بَنِي إِسْرَائِيلَ اذْكُرُوا</item>
            <item>يَا بَنِي إِسْرَائِيلَ اذْكُرُوا</item>
            <item>وَإِذِ اسْتَسْقَىٰ مُوسَىٰ</item>
            <item>إِنَّ الَّذِينَ آمَنُوا وَالَّذِينَ</item>
            <item>وَإِذْ قَتَلْتُمْ نَفْسًا</item>
            <item>وَإِذْ أَخَذْنَا مِيثَاقَ</item>
            <item>وَلَقَدْ آتَيْنَا مُوسَى</item>
            <item>قُلْ مَن كَانَ عَدُوًّا</item>
            <item>يَا أَيُّهَا الَّذِينَ آمَنُوا لَا</item>
            <item>وَقَالَتِ الْيَهُودُ لَيْسَتِ النَّصَارَىٰ</item>
            <item>يَا بَنِي إِسْرَائِيلَ اذْكُرُوا</item>
            <item>وَمَن يَرْغَبُ عَن مِّلَّةِ</item>
        </string-array>

        <string-array name="juz_1">
            <item>سَيَقُولُ السُّفَهَاءُ مِنَ النَّاسِ</item>
            <item>وَلِكُلٍّ وِجْهَةٌ هُوَ مُوَلِّيهَا</item>
            <item>يَا أَيُّهَا الَّذِينَ آمَنُوا اسْتَعِينُوا</item>
            <item>إِنَّ فِي خَلْقِ السَّمَاوَاتِ</item>
            <item>يَا أَيُّهَا النَّاسُ كُلُوا مِمَّا</item>
            <item>لَّيْسَ الْبِرَّ أَن تُوَلُّوا</item>
            <item>يَا أَيُّهَا الَّذِينَ آمَنُوا كُتِبَ</item>
            <item>يَسْأَلُونَكَ عَنِ الْأَهِلَّةِ</item>
            <item>الْحَجُّ أَشْهُرٌ مَّعْلُومَاتٌ</item>
            <item>سَلْ بَنِي إِسْرَائِيلَ كَمْ</item>
            <item>يَسْأَلُونَكَ عَنِ الشَّهْرِ الْحَرَامِ</item>
            <item>وَيَسْأَلُونَكَ عَنِ الْمَحِيضِ</item>
            <item>الطَّلَاقُ مَرَّتَانِ ۖ فَإِمْسَاكٌ</item>
            <item>وَإِذَا طَلَّقْتُمُ النِّسَاءَ</item>
            <item>لَّا جُنَاحَ عَلَيْكُمْ إِن طَلَّقْتُمُ</item>
            <item>أَلَمْ تَرَ إِلَى الَّذِينَ خَرَجُوا</item>
            <item>فَلَمَّا فَصَلَ طَالُوتُ بِالْجُنُودِ</item>
        </string-array>

        <string-array name="juz_2">
            <item>يَا أَيُّهَا الَّذِينَ آمَنُوا أَنفِقُوا</item>
            <item>أَلَمْ تَرَ إِلَى الَّذِي حَاجَّ</item>
            <item>مَّثَلُ الَّذِينَ يُنفِقُونَ</item>
            <item>يَا أَيُّهَا الَّذِينَ آمَنُوا أَنفِقُوا</item>
            <item>الَّذِينَ يُنفِقُونَ أَمْوَالَهُم</item>
            <item>يَا أَيُّهَا الَّذِينَ آمَنُوا إِذَا تَدَايَنتُم</item>
            <item>لِّلَّهِ مَا فِي السَّمَاوَاتِ</item>
            <item>الم</item>
            <item>إِنَّ الَّذِينَ كَفَرُوا لَن تُغْنِيَ</item>
            <item>إِنَّ الَّذِينَ يَكْفُرُونَ بِآيَاتِ</item>
            <item>قُلْ إِن كُنتُمْ تُحِبُّونَ</item>
            <item>وَإِذْ قَالَتِ الْمَلَائِكَةُ</item>
            <item>إِذْ قَالَ اللَّهُ يَا عِيسَىٰ</item>
            <item>قُلْ يَا أَهْلَ الْكِتَابِ تَعَالَوْا</item>
            <item>وَقَالَت طَّائِفَةٌ مِّنْ أَهْلِ الْكِتَابِ</item>
            <item>وَإِذْ أَخَذَ اللَّهُ مِيثَاقَ النَّبِيِّينَ</item>
        </string-array>

        <string-array name="juz_3">
            <item>لَن تَنَالُوا الْبِرَّ حَتَّىٰ تُنفِقُوا</item>
            <item>يَا أَيُّهَا الَّذِينَ آمَنُوا اتَّقُوا اللَّهَ</item>
            <item>كُنتُمْ خَيْرَ أُمَّةٍ أُخْرِجَتْ لِلنَّاسِ</item>
            <item>وَإِذْ غَدَوْتَ مِنْ أَهْلِكَ</item>
            <item>يَا أَيُّهَا الَّذِينَ آمَنُوا لَا تَأْكُلُوا</item>
            <item>وَمَا مُحَمَّدٌ إِلَّا رَسُولٌ</item>
            <item>يَا أَيُّهَا الَّذِينَ آمَنُوا إِن تُطِيعُوا</item>
            <item>يَا أَيُّهَا الَّذِينَ آمَنُوا لَا تَكُونُوا</item>
            <item>الَّذِينَ اسْتَجَابُوا لِلَّهِ وَالرَّسُولِ</item>
            <item>لَّقَدْ سَمِعَ اللَّهُ قَوْلَ الَّذِينَ قَالُوا</item>
            <item>إِنَّ فِي خَلْقِ السَّمَاوَاتِ وَالْأَرْضِ</item>
            <item>يَا أَيُّهَا النَّاسُ اتَّقُوا رَبَّكُمُ</item>
            <item>يُوصِيكُمُ اللَّهُ فِي أَوْلَادِكُمْ</item>
            <item>وَاللَّاتِي يَأْتِينَ الْفَاحِشَةَ مِن</item>
            <item>حُرِّمَتْ عَلَيْكُمْ أُمَّهَاتُكُمْ</item>
        </string-array>

        <string-array name="juz_4">
            <item>يُرِيدُ اللَّهُ لِيُبَيِّنَ لَكُمْ</item>
            <item>الرِّجَالُ قَوَّامُونَ عَلَى النِّسَاءِ</item>
            <item>يَا أَيُّهَا الَّذِينَ آمَنُوا لَا تَقْرَبُوا</item>
            <item>أَلَمْ تَرَ إِلَى الَّذِينَ أُوتُوا نَصِيبًا</item>
            <item>أَلَمْ تَرَ إِلَى الَّذِينَ يَزْعُمُونَ أَنَّهُمْ</item>
            <item>يَا أَيُّهَا الَّذِينَ آمَنُوا خُذُوا</item>
            <item>أَلَمْ تَرَ إِلَى الَّذِينَ قِيلَ لَهُمْ</item>
            <item>فَمَا لَكُمْ فِي الْمُنَافِقِينَ</item>
            <item>وَمَا كَانَ لِمُؤْمِنٍ أَن يَقْتُلَ</item>
            <item>إِنَّ الَّذِينَ تَوَفَّاهُمُ الْمَلَائِكَةُ</item>
            <item>وَإِذَا ضَرَبْتُمْ فِي الْأَرْضِ</item>
            <item>إِنَّا أَنزَلْنَا إِلَيْكَ الْكِتَابَ</item>
            <item>وَلَوْلَا فَضْلُ اللَّهِ عَلَيْكَ وَرَحْمَتُهُ</item>
            <item>إِنَّ اللَّهَ لَا يَغْفِرُ أَن يُشْرَكَ</item>
            <item>وَيَسْتَفْتُونَكَ فِي النِّسَاءِ</item>
            <item>يَا أَيُّهَا الَّذِينَ آمَنُوا كُونُوا قَوَّامِينَ</item>
            <item>إِنَّ الْمُنَافِقِينَ يُخَادِعُونَ اللَّهَ</item>
        </string-array>

        <string-array name="juz_5">
            <item>يَسْأَلُكَ أَهْلُ الْكِتَابِ أَن</item>
            <item>إِنَّا أَوْحَيْنَا إِلَيْكَ كَمَا</item>
            <item>لَّن يَسْتَنكِفَ الْمَسِيحُ أَن يَكُونَ</item>
            <item>يَا أَيُّهَا الَّذِينَ آمَنُوا أَوْفُوا بِالْعُقُودِ</item>
            <item>يَا أَيُّهَا الَّذِينَ آمَنُوا إِذَا قُمْتُمْ</item>
            <item>وَلَقَدْ أَخَذَ اللَّهُ مِيثَاقَ</item>
            <item>وَإِذْ قَالَ مُوسَىٰ لِقَوْمِهِ</item>
            <item>وَاتْلُ عَلَيْهِمْ نَبَأَ ابْنَيْ</item>
            <item>يَا أَيُّهَا الَّذِينَ آمَنُوا اتَّقُوا اللَّهَ</item>
            <item>إِنَّا أَنزَلْنَا التَّوْرَاةَ فِيهَا</item>
            <item>يَا أَيُّهَا الَّذِينَ آمَنُوا لَا تَتَّخِذُوا</item>
            <item>يَا أَيُّهَا الَّذِينَ آمَنُوا لَا تَتَّخِذُوا</item>
            <item>يَا أَيُّهَا الرَّسُولُ بَلِّغْ مَا أُنزِلَ</item>
            <item>لُعِنَ الَّذِينَ كَفَرُوا مِن</item>
        </string-array>

        <string-array name="juz_6">
            <item>يَا أَيُّهَا الَّذِينَ آمَنُوا لَا تُحَرِّمُوا</item>
            <item>يَا أَيُّهَا الَّذِينَ آمَنُوا لَيَبْلُوَنَّكُمُ</item>
            <item>يَا أَيُّهَا الَّذِينَ آمَنُوا لَا تَسْأَلُوا</item>
            <item>يَوْمَ يَجْمَعُ اللَّهُ الرُّسُلَ</item>
            <item>وَإِذْ قَالَ اللَّهُ يَا عِيسَى ابْنَ مَرْيَمَ</item>
            <item>الْحَمْدُ لِلَّهِ الَّذِي خَلَقَ السَّمَاوَاتِ</item>
            <item>قُلْ سِيرُوا فِي الْأَرْضِ ثُمَّ انظُرُوا</item>
            <item>وَمَنْ أَظْلَمُ مِمَّنِ افْتَرَىٰ</item>
            <item>قَدْ خَسِرَ الَّذِينَ كَذَّبُوا</item>
            <item>وَلَقَدْ أَرْسَلْنَا إِلَىٰ أُمَمٍ</item>
            <item>وَأَنذِرْ بِهِ الَّذِينَ يَخَافُونَ</item>
            <item>قُلْ إِنِّي نُهِيتُ أَنْ أَعْبُدَ</item>
            <item>وَهُوَ الْقَاهِرُ فَوْقَ عِبَادِهِ</item>
            <item>قُلْ أَنَدْعُو مِن دُونِ اللَّهِ</item>
            <item>وَتِلْكَ حُجَّتُنَا آتَيْنَاهَا إِبْرَاهِيمَ</item>
            <item>وَمَا قَدَرُوا اللَّهَ حَقَّ قَدْرِهِ</item>
            <item>إِنَّ اللَّهَ فَالِقُ الْحَبِّ وَالنَّوَىٰ</item>
            <item>بَدِيعُ السَّمَاوَاتِ وَالْأَرْضِ</item>
        </string-array>

        <string-array name="juz_7">
            <item>وَلَوْ أَنَّنَا نَزَّلْنَا إِلَيْهِمُ الْمَلَائِكَةَ</item>
            <item>أَوَمَن كَانَ مَيْتًا فَأَحْيَيْنَاهُ</item>
            <item>يَا مَعْشَرَ الْجِنِّ وَالْإِنسِ</item>
            <item>وَهُوَ الَّذِي أَنشَأَ جَنَّاتٍ مَّعْرُوشَاتٍ</item>
            <item>قُل لَّا أَجِدُ فِي مَا أُوحِيَ</item>
            <item>قُلْ تَعَالَوْا أَتْلُ مَا حَرَّمَ رَبُّكُمْ</item>
            <item>وَهَـٰذَا كِتَابٌ أَنزَلْنَاهُ مُبَارَكٌ</item>
            <item>المص</item>
            <item>وَلَقَدْ خَلَقْنَاكُمْ ثُمَّ صَوَّرْنَاكُمْ</item>
            <item>يَا بَنِي آدَمَ قَدْ أَنزَلْنَا</item>
            <item>قُلْ مَنْ حَرَّمَ زِينَةَ اللَّهِ</item>
            <item>إِنَّ الَّذِينَ كَذَّبُوا بِآيَاتِنَا</item>
            <item>وَنَادَىٰ أَصْحَابُ الْأَعْرَافِ</item>
            <item>إِنَّ رَبَّكُمُ اللَّهُ الَّذِي خَلَقَ</item>
            <item>لَقَدْ أَرْسَلْنَا نُوحًا إِلَىٰ قَوْمِهِ</item>
            <item>وَإِلَىٰ عَادٍ أَخَاهُمْ هُودًا</item>
            <item>وَإِلَىٰ ثَمُودَ أَخَاهُمْ صَالِحًا</item>
            <item>وَإِلَىٰ مَدْيَنَ أَخَاهُمْ شُعَيْبًا</item>
        </string-array>

        <string-array name="juz_8">
            <item>وَمَا أَرْسَلْنَا فِي قَرْيَةٍ مِّن نَّبِيٍّ</item>
            <item>أَوَلَمْ يَهْدِ لِلَّذِينَ يَرِثُونَ</item>
            <item>قَالَ الْمَلَأُ مِن قَوْمِ فِرْعَوْنَ</item>
            <item>وَقَالَ الْمَلَأُ مِن قَوْمِ فِرْعَوْنَ</item>
            <item>وَلَقَدْ أَخَذْنَا آلَ فِرْعَوْنَ</item>
            <item>وَوَاعَدْنَا مُوسَىٰ ثَلَاثِينَ لَيْلَةً</item>
            <item>وَاتَّخَذَ قَوْمُ مُوسَىٰ مِن بَعْدِهِ</item>
            <item>إِنَّ الَّذِينَ اتَّخَذُوا الْعِجْلَ</item>
            <item>قُلْ يَا أَيُّهَا النَّاسُ إِنِّي رَسُولُ</item>
            <item>وَاسْأَلْهُمْ عَنِ الْقَرْيَةِ الَّتِي</item>
            <item>وَإِذْ أَخَذَ رَبُّكَ مِن بَنِي آدَمَ</item>
            <item>وَالَّذِينَ كَذَّبُوا بِآيَاتِنَا</item>
            <item>هُوَ الَّذِي خَلَقَكُم مِّن نَّفْسٍ</item>
            <item>يَسْأَلُونَكَ عَنِ الْأَنفَالِ</item>
            <item>إِذْ يُغَشِّيكُمُ النُّعَاسَ أَمَنَةً</item>
            <item>يَا أَيُّهَا الَّذِينَ آمَنُوا أَطِيعُوا</item>
            <item>يَا أَيُّهَا الَّذِينَ آمَنُوا إِن تَتَّقُوا</item>
            <item>قُل لِّلَّذِينَ كَفَرُوا إِن يَنتَهُوا</item>
        </string-array>

        <string-array name="juz_9">
            <item>يَا أَيُّهَا الَّذِينَ آمَنُوا إِذَا لَقِيتُمْ</item>
            <item>إِذْ يَقُولُ الْمُنَافِقُونَ وَالَّذِينَ فِي</item>
            <item>وَلَا يَحْسَبَنَّ الَّذِينَ كَفَرُوا</item>
            <item>يَا أَيُّهَا النَّبِيُّ حَرِّضِ الْمُؤْمِنِينَ</item>
            <item>يَا أَيُّهَا النَّبِيُّ قُل لِّمَن فِي</item>
            <item>بَرَاءَةٌ مِّنَ اللَّهِ وَرَسُولِهِ</item>
            <item>كَيْفَ يَكُونُ لِلْمُشْرِكِينَ عَهْدٌ</item>
            <item>مَا كَانَ لِلْمُشْرِكِينَ أَن يَعْمُرُوا</item>
            <item>لَقَدْ نَصَرَكُمُ اللَّهُ فِي مَوَاطِنَ</item>
            <item>وَقَالَتِ الْيَهُودُ عُزَيْرٌ ابْنُ</item>
            <item>يَا أَيُّهَا الَّذِينَ آمَنُوا مَا لَكُمْ</item>
            <item>عَفَا اللَّهُ عَنكَ لِمَ أَذِنتَ</item>
            <item>إِنَّمَا الصَّدَقَاتُ لِلْفُقَرَاءِ</item>
            <item>الْمُنَافِقُونَ وَالْمُنَافِقَاتُ بَعْضُهُم</item>
            <item>يَا أَيُّهَا النَّبِيُّ جَاهِدِ الْكُفَّارَ</item>
            <item>فَرِحَ الْمُخَلَّفُونَ بِمَقْعَدِهِمْ</item>
            <item>وَجَاءَ الْمُعَذِّرُونَ مِنَ الْأَعْرَابِ</item>
        </string-array>

        <string-array name="juz_10">
            <item>وَالسَّابِقُونَ الْأَوَّلُونَ مِنَ</item>
            <item>إِنَّ اللَّهَ اشْتَرَىٰ مِنَ الْمُؤْمِنِينَ</item>
            <item>يَا أَيُّهَا الَّذِينَ آمَنُوا اتَّقُوا اللَّهَ</item>
            <item>يَا أَيُّهَا الَّذِينَ آمَنُوا قَاتِلُوا</item>
            <item>الر ۚ تِلْكَ آيَاتُ الْكِتَابِ الْحَكِيمِ</item>
            <item>وَلَوْ يُعَجِّلُ اللَّهُ لِلنَّاسِ الشَّرَّ</item>
            <item>وَإِذَا أَذَقْنَا النَّاسَ رَحْمَةً مِّن بَعْدِ</item>
            <item>قُلْ مَن يَرْزُقُكُم مِّنَ السَّمَاءِ</item>
            <item>وَإِن كَذَّبُوكَ فَقُل لِّي عَمَلِي</item>
            <item>وَلَوْ أَنَّ لِكُلِّ نَفْسٍ ظَلَمَتْ</item>
            <item>وَمَا تَكُونُ فِي شَأْنٍ وَمَا تَتْلُو</item>
            <item>وَاتْلُ عَلَيْهِمْ نَبَأَ نُوحٍ</item>
            <item>فَمَا آمَنَ لِمُوسَىٰ إِلَّا ذُرِّيَّةٌ</item>
            <item>وَلَقَدْ بَوَّأْنَا بَنِي إِسْرَائِيلَ</item>
            <item>قُلْ يَا أَيُّهَا النَّاسُ إِن كُنتُمْ</item>
            <item>الر ۚ كِتَابٌ أُحْكِمَتْ آيَاتُهُ</item>
        </string-array>

        <string-array name="juz_11">
            <item>وَلَئِنْ أَذَقْنَا الْإِنسَانَ مِنَّا رَحْمَةً</item>
            <item>وَلَقَدْ أَرْسَلْنَا نُوحًا إِلَىٰ قَوْمِهِ</item>
            <item>وَأُوحِيَ إِلَىٰ نُوحٍ أَنَّهُ لَن يُؤْمِنَ</item>
            <item>وَإِلَىٰ عَادٍ أَخَاهُمْ هُودًا</item>
            <item>وَإِلَىٰ ثَمُودَ أَخَاهُمْ صَالِحًا</item>
            <item>وَلَقَدْ جَاءَتْ رُسُلُنَا إِبْرَاهِيمَ</item>
            <item>وَإِلَىٰ مَدْيَنَ أَخَاهُمْ شُعَيْبًا</item>
            <item>وَلَقَدْ أَرْسَلْنَا مُوسَىٰ بِآيَاتِنَا</item>
            <item>وَلَقَدْ آتَيْنَا مُوسَى الْكِتَابَ</item>
            <item>الر ۚ تِلْكَ آيَاتُ الْكِتَابِ الْمُبِينِ</item>
            <item>لَّقَدْ كَانَ فِي يُوسُفَ وَإِخْوَتِهِ</item>
            <item>وَقَالَ الَّذِي اشْتَرَاهُ مِن مِّصْرَ</item>
            <item>وَقَالَ نِسْوَةٌ فِي الْمَدِينَةِ امْرَأَتُ</item>
            <item>وَدَخَلَ مَعَهُ السِّجْنَ فَتَيَانِ</item>
            <item>وَقَالَ الْمَلِكُ إِنِّي أَرَىٰ سَبْعَ</item>
            <item>وَقَالَ الْمَلِكُ ائْتُونِي بِهِ</item>
        </string-array>

        <string-array name="juz_12">
            <item>وَجَاءَ إِخْوَةُ يُوسُفَ فَدَخَلُوا</item>
            <item>وَلَمَّا دَخَلُوا عَلَىٰ يُوسُفَ آوَىٰ</item>
            <item>فَلَمَّا اسْتَيْأَسُوا مِنْهُ خَلَصُوا نَجِيًّا</item>
            <item>وَلَمَّا فَصَلَتِ الْعِيرُ قَالَ أَبُوهُمْ</item>
            <item>وَكَأَيِّن مِّنْ آيَةٍ فِي السَّمَاوَاتِ</item>
            <item>المر ۚ تِلْكَ آيَاتُ الْكِتَابِ</item>
            <item>للَّهُ يَعْلَمُ مَا تَحْمِلُ كُلُّ أُنثَىٰ</item>
            <item>أَفَمَن يَعْلَمُ أَنَّمَا أُنزِلَ إِلَيْكَ</item>
            <item>وَيَقُولُ الَّذِينَ كَفَرُوا لَوْلَا أُنزِلَ عَلَيْهِ</item>
            <item>وَلَقَدِ اسْتُهْزِئَ بِرُسُلٍ مِّن قَبْلِكَ</item>
            <item>وَلَقَدْ أَرْسَلْنَا رُسُلًا مِّن قَبْلِكَ</item>
            <item>الر ۚ كِتَابٌ أَنزَلْنَاهُ إِلَيْكَ</item>
            <item>وَإِذْ تَأَذَّنَ رَبُّكُمْ لَئِن</item>
            <item>وَقَالَ الَّذِينَ كَفَرُوا لِرُسُلِهِمْ</item>
            <item>وَقَالَ الشَّيْطَانُ لَمَّا قُضِيَ الْأَمْرُ</item>
            <item>أَلَمْ تَرَ إِلَى الَّذِينَ بَدَّلُوا</item>
            <item>وَإِذْ قَالَ إِبْرَاهِيمُ رَبِّ اجْعَلْ</item>
            <item>وَلَا تَحْسَبَنَّ اللَّهَ غَافِلًا</item>
            <item>الر ۚ تِلْكَ آيَاتُ الْكِتَابِ وَقُرْآنٍ مُّبِينٍ</item>
        </string-array>

        <string-array name="juz_13">
            <item>وَلَقَدْ جَعَلْنَا فِي السَّمَاءِ بُرُوجًا</item>
            <item>وَلَقَدْ خَلَقْنَا الْإِنسَانَ مِن</item>
            <item>إِنَّ الْمُتَّقِينَ فِي جَنَّاتٍ وَعُيُونٍ</item>
            <item>فَلَمَّا جَاءَ آلَ لُوطٍ الْمُرْسَلُونَ</item>
            <item>وَلَقَدْ كَذَّبَ أَصْحَابُ الْحِجْرِ</item>
            <item>أَتَىٰ أَمْرُ اللَّهِ فَلَا تَسْتَعْجِلُوهُ</item>
            <item>هُوَ الَّذِي أَنزَلَ مِنَ السَّمَاءِ مَاءً</item>
            <item>إِلَـٰهُكُمْ إِلَـٰهٌ وَاحِدٌ</item>
            <item>قَدْ مَكَرَ الَّذِينَ مِن قَبْلِهِمْ</item>
            <item>وَقَالَ الَّذِينَ أَشْرَكُوا لَوْ شَاءَ</item>
            <item>وَالَّذِينَ هَاجَرُوا فِي اللَّهِ</item>
            <item>وَقَالَ اللَّهُ لَا تَتَّخِذُوا إِلَـٰهَيْنِ</item>
            <item>وَلَوْ يُؤَاخِذُ اللَّهُ النَّاسَ</item>
            <item>وَإِنَّ لَكُمْ فِي الْأَنْعَامِ لَعِبْرَةً</item>
            <item>وَاللَّهُ فَضَّلَ بَعْضَكُمْ عَلَىٰ</item>
            <item>وَلِلَّهِ غَيْبُ السَّمَاوَاتِ</item>
            <item>وَيَوْمَ نَبْعَثُ مِن كُلِّ أُمَّةٍ</item>
            <item>إِنَّ اللَّهَ يَأْمُرُ بِالْعَدْلِ</item>
            <item>وَإِذَا بَدَّلْنَا آيَةً مَّكَانَ آيَةٍ</item>
            <item>يَوْمَ تَأْتِي كُلُّ نَفْسٍ تُجَادِلُ</item>
            <item>إِنَّ إِبْرَاهِيمَ كَانَ أُمَّةً قَانِتًا</item>
        </string-array>

        <string-array name="juz_14">
            <item>سُبْحَانَ الَّذِي أَسْرَىٰ بِعَبْدِهِ</item>
            <item>وَيَدْعُ الْإِنسَانُ بِالشَّرِّ دُعَاءَهُ</item>
            <item>وَقَضَىٰ رَبُّكَ أَلَّا تَعْبُدُوا</item>
            <item>وَلَا تَقْتُلُوا أَوْلَادَكُمْ خَشْيَةَ</item>
            <item>وَلَقَدْ صَرَّفْنَا فِي هَـٰذَا الْقُرْآنِ</item>
            <item>وَقُل لِّعِبَادِي يَقُولُوا الَّتِي</item>
            <item>وَإِذْ قُلْنَا لِلْمَلَائِكَةِ اسْجُدُوا</item>
            <item>يَوْمَ نَدْعُو كُلَّ أُنَاسٍ</item>
            <item>أَقِمِ الصَّلَاةَ لِدُلُوكِ الشَّمْسِ</item>
            <item>وَيَسْأَلُونَكَ عَنِ الرُّوحِ</item>
            <item>وَمَا مَنَعَ النَّاسَ أَن يُؤْمِنُوا</item>
            <item>وَلَقَدْ آتَيْنَا مُوسَىٰ تِسْعَ</item>
            <item>الْحَمْدُ لِلَّهِ الَّذِي أَنزَلَ عَلَىٰ</item>
            <item>نَّحْنُ نَقُصُّ عَلَيْكَ نَبَأَهُم بِالْحَقِّ</item>
            <item>وَتَحْسَبُهُمْ أَيْقَاظًا وَهُمْ رُقُودٌ</item>
            <item>وَلَا تَقُولَنَّ لِشَيْءٍ إِنِّي</item>
            <item>وَاضْرِبْ لَهُم مَّثَلًا رَّجُلَيْنِ</item>
            <item>وَاضْرِبْ لَهُم مَّثَلَ الْحَيَاةِ الدُّنْيَا</item>
            <item>وَإِذْ قُلْنَا لِلْمَلَائِكَةِ اسْجُدُوا</item>
            <item>وَلَقَدْ صَرَّفْنَا فِي هَـٰذَا الْقُرْآنِ</item>
            <item>وَإِذْ قَالَ مُوسَىٰ لِفَتَاهُ</item>
            <item>فَانطَلَقَا حَتَّىٰ إِذَا رَكِبَا</item>
        </string-array>

        <string-array name="juz_15">
            <item>وَيَسْأَلُونَكَ عَن ذِي الْقَرْنَيْنِ</item>
            <item>أَفَحَسِبَ الَّذِينَ كَفَرُوا</item>
            <item>كهيعص</item>
            <item>وَاذْكُرْ فِي الْكِتَابِ مَرْيَمَ</item>
            <item>وَاذْكُرْ فِي الْكِتَابِ إِبْرَاهِيمَ</item>
            <item>وَاذْكُرْ فِي الْكِتَابِ مُوسَىٰ</item>
            <item>وَيَقُولُ الْإِنسَانُ أَإِذَا مَا مِتُّ</item>
            <item>أَلَمْ تَرَ أَنَّا أَرْسَلْنَا الشَّيَاطِينَ</item>
            <item>طه</item>
            <item>قَالَ رَبِّ اشْرَحْ لِي صَدْرِي</item>
            <item>مِنْهَا خَلَقْنَاكُمْ وَفِيهَا</item>
            <item>وَلَقَدْ أَوْحَيْنَا إِلَىٰ مُوسَىٰ</item>
            <item>وَلَقَدْ قَالَ لَهُمْ هَارُونُ</item>
            <item>وَيَسْأَلُونَكَ عَنِ الْجِبَالِ</item>
            <item>وَإِذْ قُلْنَا لِلْمَلَائِكَةِ اسْجُدُوا</item>
            <item>وَلَوْلَا كَلِمَةٌ سَبَقَتْ مِن</item>
        </string-array>

        <string-array name="juz_16">
            <item>اقْتَرَبَ لِلنَّاسِ حِسَابُهُمْ</item>
            <item>وَكَمْ قَصَمْنَا مِن قَرْيَةٍ كَانَتْ</item>
            <item>أَوَلَمْ يَرَ الَّذِينَ كَفَرُوا أَنَّ</item>
            <item>قُلْ مَن يَكْلَؤُكُم بِاللَّيْلِ</item>
            <item>وَلَقَدْ آتَيْنَا إِبْرَاهِيمَ رُشْدَهُ</item>
            <item>وَنُوحًا إِذْ نَادَىٰ مِن قَبْلُ</item>
            <item>فَمَن يَعْمَلْ مِنَ الصَّالِحَاتِ</item>
            <item>يَا أَيُّهَا النَّاسُ اتَّقُوا رَبَّكُمْ</item>
            <item>وَمِنَ النَّاسِ مَن يَعْبُدُ اللَّهَ</item>
            <item>إِنَّ اللَّهَ يُدْخِلُ الَّذِينَ آمَنُوا</item>
            <item>وَإِذْ بَوَّأْنَا لِإِبْرَاهِيمَ مَكَانَ</item>
            <item>وَلِكُلِّ أُمَّةٍ جَعَلْنَا مَنسَكًا</item>
            <item>أُذِنَ لِلَّذِينَ يُقَاتَلُونَ بِأَنَّهُمْ</item>
            <item>قُلْ يَا أَيُّهَا النَّاسُ إِنَّمَا أَنَا</item>
            <item>وَالَّذِينَ هَاجَرُوا فِي سَبِيلِ</item>
            <item>أَلَمْ تَرَ أَنَّ اللَّهَ سَخَّرَ لَكُم</item>
            <item>يَا أَيُّهَا النَّاسُ ضُرِبَ مَثَلٌ</item>
        </string-array>

        <string-array name="juz_17">
            <item>قَدْ أَفْلَحَ الْمُؤْمِنُونَ</item>
            <item>وَلَقَدْ أَرْسَلْنَا نُوحًا إِلَىٰ قَوْمِهِ</item>
            <item>وَقَالَ الْمَلَأُ مِن قَوْمِهِ الَّذِينَ</item>
            <item>يَا أَيُّهَا الرُّسُلُ كُلُوا مِنَ</item>
            <item>وَهُوَ الَّذِي أَنشَأَ لَكُمُ السَّمْعَ</item>
            <item>قُل رَّبِّ إِمَّا تُرِيَنِّي مَا يُوعَدُونَ</item>
            <item>سُورَةٌ أَنزَلْنَاهَا وَفَرَضْنَاهَا</item>
            <item>إِنَّ الَّذِينَ جَاءُوا بِالْإِفْكِ</item>
            <item>يَا أَيُّهَا الَّذِينَ آمَنُوا لَا تَتَّبِعُوا</item>
            <item>يَا أَيُّهَا الَّذِينَ آمَنُوا لَا تَدْخُلُوا</item>
            <item>اللَّهُ نُورُ السَّمَاوَاتِ وَالْأَرْضِ</item>
            <item>أَلَمْ تَرَ أَنَّ اللَّهَ يُسَبِّحُ لَهُ مَن فِي</item>
            <item>إِنَّمَا كَانَ قَوْلَ الْمُؤْمِنِينَ</item>
            <item>يَا أَيُّهَا الَّذِينَ آمَنُوا لِيَسْتَأْذِنكُمُ</item>
            <item>إِنَّمَا الْمُؤْمِنُونَ الَّذِينَ آمَنُوا بِاللَّهِ</item>
            <item>تَبَارَكَ الَّذِي نَزَّلَ الْفُرْقَانَ</item>
            <item>تَبَارَكَ الَّذِي إِن شَاءَ جَعَلَ</item>
        </string-array>

        <string-array name="juz_18">
            <item>وَقَالَ الَّذِينَ لَا يَرْجُونَ لِقَاءَنَا</item>
            <item>وَلَقَدْ آتَيْنَا مُوسَى الْكِتَابَ</item>
            <item>أَلَمْ تَرَ إِلَىٰ رَبِّكَ كَيْفَ مَدَّ الظِّلَّ</item>
            <item>تَبَارَكَ الَّذِي جَعَلَ فِي السَّمَاءِ</item>
            <item>طسم</item>
            <item>وَإِذْ نَادَىٰ رَبُّكَ مُوسَىٰ أَنِ</item>
            <item>قَالَ لِلْمَلَإِ حَوْلَهُ إِنَّ هَـٰذَا</item>
            <item>وَأَوْحَيْنَا إِلَىٰ مُوسَىٰ أَنْ أَسْرِ</item>
            <item>وَاتْلُ عَلَيْهِمْ نَبَأَ إِبْرَاهِيمَ</item>
            <item>كَذَّبَتْ قَوْمُ نُوحٍ الْمُرْسَلِينَ</item>
            <item>كَذَّبَتْ عَادٌ الْمُرْسَلِينَ</item>
            <item>كَذَّبَتْ ثَمُودُ الْمُرْسَلِينَ</item>
            <item>كَذَّبَتْ قَوْمُ لُوطٍ الْمُرْسَلِينَ</item>
            <item>كَذَّبَ أَصْحَابُ الْأَيْكَةِ الْمُرْسَلِينَ</item>
            <item>وَإِنَّهُ لَتَنزِيلُ رَبِّ الْعَالَمِينَ</item>
            <item>طس ۚ تِلْكَ آيَاتُ الْقُرْآنِ</item>
            <item>وَلَقَدْ آتَيْنَا دَاوُودَ وَسُلَيْمَانَ</item>
            <item>قَالَتْ يَا أَيُّهَا الْمَلَأُ أَفْتُونِي</item>
            <item>وَلَقَدْ أَرْسَلْنَا إِلَىٰ ثَمُودَ أَخَاهُمْ</item>
            <item>قُلِ الْحَمْدُ لِلَّهِ وَسَلَامٌ عَلَىٰ عِبَادِهِ</item>
        </string-array>

        <string-array name="juz_19">
            <item>وَقَالَ الَّذِينَ كَفَرُوا أَإِذَا</item>
            <item>وَيَوْمَ نَحْشُرُ مِن كُلِّ أُمَّةٍ</item>
            <item>طسم</item>
            <item>وَلَمَّا بَلَغَ أَشُدَّهُ وَاسْتَوَىٰ</item>
            <item>وَلَمَّا تَوَجَّهَ تِلْقَاءَ مَدْيَنَ</item>
            <item>فَلَمَّا قَضَىٰ مُوسَى الْأَجَلَ</item>
            <item>وَلَقَدْ آتَيْنَا مُوسَى الْكِتَابَ</item>
            <item>وَلَقَدْ وَصَّلْنَا لَهُمُ الْقَوْلَ</item>
            <item>أَفَمَن وَعَدْنَاهُ وَعْدًا حَسَنًا</item>
            <item>إِنَّ قَارُونَ كَانَ مِن قَوْمِ مُوسَىٰ</item>
